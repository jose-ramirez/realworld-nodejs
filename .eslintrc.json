{
    "extends": "strongloop",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "rules": {
        "indent": [2, 4, { "SwitchCase": 1 }],
        "space-before-function-paren": [2, {
            "asyncArrow": "always"
        }],
        "comma-dangle": ["error", "only-multiline"]
    }
}